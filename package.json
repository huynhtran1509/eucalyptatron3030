{
  "name": "eucalyptatron3030",
  "version": "2.0.0",
  "description": "wta bot experiment",
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "ntl",
    "transpile": "babel src/ --out-dir lib/",
    "create": "npm run transpile && claudia create --version dev --region us-east-1 --api-module lib/bot",
    "deploy": "npm run transpile && claudia update --version dev && claudia set-version --version production",
    "deploy-dev": "npm run transpile && claudia update --version dev",
    "reassign-dev": "claudia set-version --version dev",
    "release": "claudia set-version --version production",
    "call-dev": "claudia test-lambda --version dev",
    "call-production": "claudia test-lambda --version production"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "author": "James Fishwick james.fishwick@willowtreeapps.com",
  "license": "ISC",
  "dependencies": {
    "alexa-message-builder": "^1.1.0",
    "aws-lambda-invoke": "^3.0.0",
    "aws-sdk": "^2.39.0",
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "claudia-bot-builder": "^2.15.0",
    "huh": "^2.0.1",
    "immutable": "^3.8.1",
    "moment": "^2.18.1",
    "promise-delay": "^2.1.0",
    "ramda": "^0.23.0",
    "redux": "^3.6.0",
    "stack-trace": "0.0.9"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "claudia": "^2.10.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-flowtype-errors": "^3.0.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.44.2",
    "ntl": "^1.2.0"
  }
}